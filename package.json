{
  "name": "FreeBoardGame.org",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "autoprecommit": "yarn run fix && yarn run buildtest",
    "autopre": "yarn run autoprecommit",
    "buildtest": "yarn run server:build && yarn run test",
    "dev": "run-s server:build dev:watch",
    "dev:watch": "webpack-dev-server --progress --config webpack.dev.js",
    "fix": "yarn run lint",
    "precommit:format": "pretty-quick --staged",
    "lint": "eslint --fix --ext .ts,.tsx src/ @types/",
    "prod": "run-s prod:build server",
    "prod:buildquietly": "run-s server:buildquietly webpack:buildquietly",
    "prod:build": "run-s server:build webpack:build",
    "pre": "yarn run precommit",
    "server": "yarn run server:both",
    "server:bgio": "node server-build/server_bgio.js",
    "server:fbg": "node server-build/server_fbg.js",
    "server:both": "run-p server:bgio server:fbg",
    "server:build": "webpack --progress --config webpack.server.js",
    "server:buildquietly": "webpack --config webpack.server.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:watchverbose": "jest --watch --verbose=true --detectOpenHandles",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "test:coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "webpack:build": "webpack --progress --config webpack.prod.js",
    "webpack:buildquietly": "webpack --config webpack.prod.js",
    "verify-lint": "eslint --ext .ts,.tsx src/ @types/"
  },
  "pre-commit": [
    "precommit"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:freeboardgame/FreeBoardGame.org.git"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/freeboardgame/FreeBoardGame.org/issues"
  },
  "homepage": "https://github.com/freeboardgame/FreeBoardGame.org",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.13",
    "@types/koa": "^2.0.48",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-static": "^4.0.1",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.3",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "koa-helmet": "^4.0.0",
    "nodemon": "^1.18.9",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.1",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@freeboardgame.org/boardgame.io": "^0.0.8",
    "@freeboardgame.org/i18n": "^0.0.3",
    "@koa/cors": "3.0.0",
    "@material-ui/core": "3.3.0",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@tweenjs/tween.js": "^17.4.0",
    "@types/enzyme": "^3.1.16",
    "@types/koa-router": "^7.0.40",
    "@types/mustache": "^0.8.32",
    "@types/rrc": "^0.10.5",
    "@types/serialize-javascript": "^1.5.0",
    "@types/shuffle-array": "^0.0.28",
    "@types/superagent": "^4.1.1",
    "chess.js": "^0.10.2",
    "copy-to-clipboard": "^3.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "empty-module": "^0.0.2",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "koa": "^2.7.0",
    "koa-no-cache": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-socket-2": "^1.1.0",
    "koa-static": "^5.0.0",
    "mongo": "^0.1.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "qrcode.react": "^0.9.3",
    "raw-loader": "^3.0.0",
    "react": "^16.7.0",
    "react-async-bootstrapper": "^2.1.1",
    "react-async-component": "^2.0.0",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.7",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.1",
    "rrc": "^0.10.1",
    "serialize-javascript": "^1.7.0",
    "shortid": "^2.2.14",
    "shuffle-array": "^1.0.1",
    "socket.io-client": "^2.2.0",
    "superagent": "^5.0.6",
    "three": "^0.105.2",
    "webpack-shell-plugin-next": "^0.6.4",
    "workbox-sw": "^4.3.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/server_bgio.tsx",
      "!src/server_fbg.tsx",
      "!src/app.tsx",
      "!**/*.d.ts",
      "!src/games/chess/stockfish8.js",
      "!src/games/chess/chessjswrapper.ts"
    ],
    "testURL": "http://localhost",
    "setupFiles": [
      "./test/test-shim.js",
      "./test/test-setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/server-build/"
    ],
    "moduleNameMapper": {
      "\\.worker.js": "<rootDir>/test/stockfish8Mock.js",
      "\\.(css|jpg|png|svg|webp|mp3|md)$": "<rootDir>/test/empty-module.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s precommit:format verify-lint buildtest"
    }
  }
}
